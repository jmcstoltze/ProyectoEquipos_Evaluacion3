<Window x:Class="EvaluacionGUI.Views.AcercaDe"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EvaluacionGUI.Views"
        mc:Ignorable="d"
        Title="Acerca de la aplicación" Height="450" Width="900">
    <Grid>
        <Grid>
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                <TextBlock x:Name="txtDesarrollador" Text="Desarrollado por: José Contreras Stoltze" Margin="0 0 0 20"/>
                <TextBlock x:Name="txtAnio" Text="Año de desarrollo: 2024" Margin="40 0 0 20"/>
                <Button x:Name="btnVolverAlMenu" Content="Volver al menú principal" Click="btnVolverAlMenu_Click"/>
            </StackPanel>
        </Grid>
    </Grid>
</Window>
